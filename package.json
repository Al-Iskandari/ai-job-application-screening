{
  "name": "ai-job-application-screening",
  "version": "1.0.0",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:worker\"",
    "dev:server": "nodemon --exec tsx src/server.ts",
    "dev:worker": "nodemon --exec tsx src/worker.ts",

    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "start:worker": "node dist/worker.js",

    "test": "echo \"Error: no test specified\" && exit 1",
    "load:systemdocs": "nodemon --exec tsx src/script/loadSystemDocs.ts",

    "docker:build": "docker compose build",
    "docker:up": "docker compose up --remove-orphans && echo 'ðŸš€ App running at http://localhost:3000 (API) and Worker active'",
    "docker:down": "docker compose down -v",
    "docker:logs": "docker compose logs -f",
    "docker:restart": "docker compose restart"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@google/genai": "^1.22.0",
    "@supabase/supabase-js": "^2.74.0",
    "@zilliz/milvus2-sdk-node": "^2.5.12",
    "bullmq": "^5.60.0",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "firebase-admin": "^13.5.0",
    "pdf-ts": "^0.0.2",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.6.2",
    "@types/pdf-parse": "^1.1.5",
    "@types/uuid": "^10.0.0",
    "concurrently": "^9.2.1",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
